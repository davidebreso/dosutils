StdGrp		group	stdlib, stddata

stddata		segment	para public 'sldata'
stddata		ends

stdlib		segment	para public 'slcode'
stdlib		ends

slcall		macro	adrs
		db	09ah			;Far call
		dd	stdgrp:adrs		;Address of routine to call
		endm


TTOA		macro
		ifndef	sl_TTOA
stdlib		segment	para public 'slcode'
		extrn	sl_TTOA:far
stdlib		ends
		endif
		slcall	sl_TTOA
		endm

